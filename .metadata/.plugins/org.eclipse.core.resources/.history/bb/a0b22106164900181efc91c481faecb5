package com.dipta.project.constants;

/**
 * 
 * @author Saurabh.Agarwal
 *
 */
public enum EmailEvent {
	USERREGISTERED("USERREGISTERED"),
	USERFORGETPASSWORD("USERFORGETPASSWORD"),
	USERNOTIFICATION("USERNOTIFICATION"),
	PROJECTREPORT("PROJECTREPORT"),
	USERCHANGEPASSWORD("USERCHANGEPASSWORD"),
	SUBSCRIBEORDERDETAILS("SUBSCRIBEORDERDETAILS"),
	USERACTIVTAION("USERACTIVTAION"),
	TICKETNOTIFICATION("TICKETNOTIFICATION"),
	COMPANYREGISTRATIONFAIL("COMPANYREGISTRATIONFAIL"),
	COMPANYREGISTRATIONSUCCESS("COMPANYREGISTRATIONSUCCESS"),
	USERPROFILEUPDATE("USERPROFILEUPDATE"),
	CONTACTUS("CONTACTUS"),
	TICKETCREATIONSUCCESS("TICKETCREATIONSUCCESS"),
	ADDTOTICKETSUCCESS("ADDTOTICKETSUCCESS"),
	WORKFLOWRECORDSTOTICKETSUCCESS("WORKFLOWRECORDSTOTICKETSUCCESS"),
	WORKFLOWBYPASSSUCCESS("WORKFLOWBYPASSSUCCESS"),
	NEWUSEREXPOSEMAIL("NEWUSEREXPOSEMAIL");
	
	private String event;
	
	private EmailEvent(String event) {
		this.event=event;
	}
	
	public String getEvent() {
		return event;
	}
}
